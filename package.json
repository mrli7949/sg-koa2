{
  "name": "sg-koa2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "orgstart": "node bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "nodemon --watch src/index.js",
    "watch": "ts-node ./src/index.ts",
    "debugger": "nodemon  --watch ./src -e  ts,tsx  --exec node --inspect -r ts-node/register  ./src/index.ts",
    "watch-serve": "nodemon  --watch './src/**/*' -e  ts,tsx  --exec ts-node  ./src/index.ts"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "koa2-cors": "^2.0.6",
    "mysql2": "^1.6.5",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.19.6",
    "sequelize-typescript": "^1.0.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.41",
    "@types/sequelize": "^4.28.6",
    "eslint": "^5.16.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
